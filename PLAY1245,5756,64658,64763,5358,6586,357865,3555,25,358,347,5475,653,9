local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local radarSize = 200

-- Èõ∑Ëææ
local radarGui = Instance.new("ScreenGui")
radarGui.Name = "StaticRadar"
radarGui.ResetOnSpawn = false

local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, radarSize, 0, radarSize)
mainFrame.Position = UDim2.new(1, -radarSize-20, 0, 20)
mainFrame.BackgroundTransparency = 1

local bgCircle = Instance.new("Frame")
bgCircle.Size = UDim2.new(1, 0, 1, 0)
bgCircle.BackgroundColor3 = Color3.new(0.1, 0.1, 0.15)
bgCircle.BackgroundTransparency = 0.3
Instance.new("UICorner", bgCircle).CornerRadius = UDim.new(1, 0)

local centerDot = Instance.new("Frame")
centerDot.Size = UDim2.new(0, 8, 0, 8)
centerDot.Position = UDim2.new(0.5, -4, 0.5, -4)
centerDot.BackgroundColor3 = Color3.new(0, 1, 0)
centerDot.ZIndex = 2
Instance.new("UICorner", centerDot).CornerRadius = UDim.new(1, 0)

bgCircle.Parent = mainFrame
centerDot.Parent = mainFrame
mainFrame.Parent = radarGui
radarGui.Parent = player.PlayerGui

RunService.Heartbeat:Connect(function()
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    
    for _, child in ipairs(bgCircle:GetChildren()) do
        if child:IsA("Frame") and child ~= centerDot then
            child:Destroy()
        end
    end
    
    local root = character.HumanoidRootPart
    local selfPos = root.Position
    local selfCF = root.CFrame
   
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= player then
            local targetChar = p.Character
            if targetChar and targetChar:FindFirstChild("HumanoidRootPart") then
                local targetPos = targetChar.HumanoidRootPart.Position
                local offset = (targetPos - selfPos)
                local distance = offset.Magnitude
                
                if distance <= 150 then
                    local relative = selfCF:VectorToObjectSpace(offset)
                    local angle = math.atan2(-relative.X, relative.Z)
                    local radius = (distance / 150) * (radarSize/2 - 25)
                    
                    local posX = math.sin(angle) * radius
                    local posY = -math.cos(angle) * radius
                    
                    local dot = Instance.new("Frame")
                    dot.Size = UDim2.new(0, 8, 0, 8)
                    dot.Position = UDim2.new(
                        0.5 + posX/(radarSize/2), 
                        -4, 
                        0.5 + posY/(radarSize/2), 
                        -4
                    )
                    dot.BackgroundColor3 = Color3.new(1, 1 - distance/150, 0)
                    Instance.new("UICorner", dot).CornerRadius = UDim.new(1, 0)
                    dot.Parent = bgCircle
                end
            end
        end
    end
end)

	--game.Players.LocalPlayer:Kick()
end)
